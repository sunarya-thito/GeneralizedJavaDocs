        "see": [
          "\u003ca href\u003d\"https://docs.oracle.com/en/java/javase/16/docs/api/java.desktop/javax/swing/ListSelectionModel.html#getSelectionMode()\"\u003e\u003ccode\u003eListSelectionModel.getSelectionMode()\u003c/code\u003e\u003c/a\u003e"
        ],
